@{
    ViewData["Title"] = "Alistamiento de Material";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1><a href="~/ComfasAlistamientoMaterial/Individual" style="text-decoration:none;">Alistamiento de Material</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="UnidadNaval" class="col-3 col-form-label"><a href="~/UnidadNaval">Unidad</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbUnidadNaval" name="cbUnidadNaval"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="CapacidadOperativa" class="col-3 col-form-label"><a href="~/CapacidadOperativa">Capacidad Operativa</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbCapacidadOperativa" name="cbCapacidadOperativa"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="CapacidadIntrinseca1" class="col-3 col-form-label">Capacidad Intrinseca I Nivel</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtCapacidadIntrinseca1" name="txtCapacidadIntrinseca1" placeholder="Capacidad Intrinseca I Nivel" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="PonderacionCapacidad1" class="col-3 col-form-label">Ponderación Capacidad Intrinseca I Nivel</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtPonderacionCapacidad1" name="txtPonderacionCapacidad1" placeholder="Ponderación Capacidad Intrinseca I Nivel" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Subclasificacion2" class="col-3 col-form-label">Sub Clasificación II Nivel</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtSubclasificacion2" name="txtSubclasificacion2" placeholder="Sub Clasificación II Nivel" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="PonderacionCapacidad2" class="col-3 col-form-label">Ponderación Capacidad Intrinseca II Nivel</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtPonderacionCapacidad2" name="txtPonderacionCapacidad2" placeholder="Ponderación Capacidad Intrinseca II Nivel" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Subclasificacion3" class="col-3 col-form-label">Sub Clasificación III Nivel</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtSubclasificacion3" name="txtSubclasificacion3" placeholder="Sub Clasificación III Nivel" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="PonderacionCapacidad3" class="col-3 col-form-label">Ponderación Capacidad Intrinseca III Nivel</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtPonderacionCapacidad3" name="txtPonderacionCapacidad3" placeholder="Ponderación Capacidad Intrinseca III Nivel" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Requerido" class="col-3 col-form-label">Requerido</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtRequerido" name="txtRequerido" placeholder="Requerido" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Operativo" class="col-3 col-form-label">Operativo</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtOperativo" name="txtOperativo" placeholder="Operativo" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NivelAlistamientoParcial" class="col-3 col-form-label">Nivel de Alistamiento Parcial</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtNivelAlistamientoParcial" name="txtNivelAlistamientoParcial" placeholder="Nivel de Alistamiento Parcial" required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/ComfasAlistamientoMaterial">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="editar" style="display:none;">
    <form class="form-actualizacion" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <input type="hidden" id="txtCodigo" />
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Editar</h4>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="UnidadNaval" class="col-3 col-form-label"><a href="~/UnidadNaval">Unidad</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbUnidadNavale" name="cbUnidadNavale"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="CapacidadOperativa" class="col-3 col-form-label"><a href="~/CapacidadOperativa">Capacidad Operativa</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbCapacidadOperativae" name="cbCapacidadOperativae"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="CapacidadIntrinseca1" class="col-3 col-form-label">Capacidad Intrinseca I Nivel</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtCapacidadIntrinseca1e" name="txtCapacidadIntrinseca1e" placeholder="Capacidad Intrinseca I Nivel" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="PonderacionCapacidad1" class="col-3 col-form-label">Ponderación Capacidad Intrinseca I Nivel</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtPonderacionCapacidad1e" name="txtPonderacionCapacidad1e" placeholder="Ponderación Capacidad Intrinseca I Nivel" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Subclasificacion2" class="col-3 col-form-label">Sub Clasificación II Nivel</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtSubclasificacion2e" name="txtSubclasificacion2e" placeholder="Sub Clasificación II Nivel" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="PonderacionCapacidad2" class="col-3 col-form-label">Ponderación Capacidad Intrinseca II Nivel</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtPonderacionCapacidad2e" name="txtPonderacionCapacidad2e" placeholder="Ponderación Capacidad Intrinseca II Nivel" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Subclasificacion3" class="col-3 col-form-label">Sub Clasificación III Nivel</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtSubclasificacion3e" name="txtSubclasificacion3e" placeholder="Sub Clasificación III Nivel" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="PonderacionCapacidad3" class="col-3 col-form-label">Ponderación Capacidad Intrinseca III Nivel</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtPonderacionCapacidad3e" name="txtPonderacionCapacidad3e" placeholder="Ponderación Capacidad Intrinseca III Nivel" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Requerido" class="col-3 col-form-label">Requerido</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtRequeridoe" name="txtRequeridoe" placeholder="Requerido" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Operativo" class="col-3 col-form-label">Operativo</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtOperativoe" name="txtOperativoe" placeholder="Operativo" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NivelAlistamientoParcial" class="col-3 col-form-label">Nivel de Alistamiento Parcial</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtNivelAlistamientoParciale" name="txtNivelAlistamientoParciale" placeholder="Nivel de Alistamiento Parcial" required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/ComfasAlistamientoMaterial">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="listar" style="overflow:scroll">
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaComfasAlistamientoMaterial()" />
    <div class="dropdown">
        <button class="dropdown-toggle" type="button" id="dropdownMenuButton" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            Modos de Carga :
        </button>
        <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
            <a class="dropdown-item" href="~/ComfasAlistamientoMaterial">Masiva</a>
            <a class="dropdown-item" href="~/ComfasAlistamientoMaterial/Individual">Individual</a>
        </div>
    </div>
    <table class="table table-striped" id="tblComfasAlistamientoMaterial" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;overflow:scroll">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>Unidad</th>
                <th>Capacidad Operativa</th>
                <th>Capacidad Intrinseca I Nivel</th>
                <th>Ponderación Capacidad Intrinseca I Nivel</th>
                <th>Sub Clasificación II Nivel</th>
                <th>Ponderación Capacidad Intrinseca II Nivel</th>
                <th>Sub Clasificación III Nivel</th>
                <th>Ponderación Capacidad Intrinseca III Nivel</th>
                <th>Requerido</th>
                <th>Operativo</th>
                <th>Nivel de Alistamiento Parcial</th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

@section Scripts{
    <script src="/js/comfas/comfas-alistamientomaterial.js" asp-append-version="true"></script>
}
