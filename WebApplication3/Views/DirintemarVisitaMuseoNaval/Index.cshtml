@{
    ViewData["Title"] = "Formato";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-sm-12">
        <h1><a href="~/DirintemarVisitaMuseoNaval" style="text-decoration:none;">Visita Registrada a los Museos Navales</a></h1>
        <div class="card">
            <div class="card-header">
                <div class="card-header">
                    <a href="~/DirintemarVisitaMuseoNaval/Individual" class="btn btn-primary">Ir Carga Individual</a>
                    <a href="~/DirintemarVisitaMuseoNaval/DownloadFile" class="btn btn-primary">Descargar Formato</a>
                </div>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-sm-4">
                        <input class="form-control" type="file" id="inputExcel" />
                    </div>
                    <div class="col-sm-4">
                        <button class="btn btn-warning" onclick="mostrarDatos()">Vista Previa</button>
                        <button class="btn btn-success" onclick="enviarDatos()">Guardar Formato</button>
                    </div>
                    <div class="col-sm-4">
                        <label>Numero de Registros : </label><span id="NumRegistros"></span>
                    </div>
                </div>
                <hr />
                <div class="row" style="overflow:scroll">
                    <div class="col-sm-12">
                        <table class="table table-striped" id="tbData" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;">
                            <thead>
                                <tr class="colorbg">
                                    <th>Código</th>
                                    <th>Museo Naval</th>
                                    <th>Periodo Visita Museo Naval </th>
                                    <th>Niños</th>
                                    <th>Adultos</th>
                                    <th>Estudiante Escolar </th>
                                    <th>Estudiante Universitario</th>
                                    <th>Docente </th>
                                    <th>Militar</th>
                                    <th>Familia Naval Adulto </th>
                                    <th>Familia Naval Niño </th>
                                    <th>Persona Discapacitada </th>
                                    <th>Adultos Civil Mayor </th>
                                    <th>Extranjera </th>
                                    <th>Otros Extranjero</th>
                                    <th>Noches Lima </th>
                                    <th>Total Visita</th>
                                    <th>Racaudación Total</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
        </div>

    </div>
</div>
@section Scripts{
    <script src="/js/dirintemar/dirintemar-actividadcultural.js" asp-append-version="true"></script>
}