@{
    ViewData["Title"] = "Direccion Inspeccion Auditoria Supervision";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1><a href="~/DicapiDireccionInspeccionAuditoriaSupervision/Individual" style="text-decoration:none;">Direccion de Inspecciones, Auditorias y Supervisiones</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>
            <div class="form-group row col-md-6">
                <label for="carga" class="col-3 col-form-label">Carga</label>
                <div class="form-group row col-md-6">
                    <div class="col"><select class="form-select-sm form-control-sm" id="cargasR" name="cargasR"></select></div>
                </div>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="Numero" class="col-3 col-form-label">N° Nombramiento</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtNumero" name="txtNumero" placeholder="N° Nombramiento" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaInspec" class="col-3 col-form-label">Fecha de Inspeccion</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaInspec" name="txtFechaInspec" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Capitania" class="col-3 col-form-label"><a href="~/Capitania">Capitania</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbCapitania" name="cbCapitania"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Inspect1" class="col-3 col-form-label">1° Inspector</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtInspect1" name="txtInspect1" placeholder="1° Inspector" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Inspect2" class="col-3 col-form-label">2° Inspector</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtInspect2" name="txtInspect2" placeholder="2° Inspector" required>
                    </div>
                </div>
            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DicapiDireccionInspeccionAuditoriaSupervision">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="editar" style="display:none;">
    <form class="form-actualizacion" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <input type="hidden" id="txtCodigo" />
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Editar</h4>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="Numero" class="col-3 col-form-label">N° Nombramiento</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtNumeroe" name="txtNumeroe" placeholder="N° Nombramiento" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaInspec" class="col-3 col-form-label">Fecha de Inspeccion</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaInspece" name="txtFechaInspece" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Capitania" class="col-3 col-form-label"><a href="~/Capitania">Capitania</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbCapitaniae" name="cbCapitaniae"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Inspect1" class="col-3 col-form-label">1° Inspector</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtInspect1e" name="txtInspect1e" placeholder="1° Inspector" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Inspect2" class="col-3 col-form-label">2° Inspector</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtInspect2e" name="txtInspect2e" placeholder="2° Inspector" required>
                    </div>
                </div>
                <div class="row mt-2">
                    <div class="col-md-6 text-center">
                        <button type="submit" class="btn btn-primary">Grabar</button>
                    </div>
                    <div class="col-md-6 text-center">
                        <a class="btn btn-danger" href="~/DicapiDireccionInspeccionAuditoriaSupervision">Cancelar</a>
                    </div>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="listar" style="overflow:scroll">
      <p><a href="~/DicapiDireccionInspeccionAuditoriaSupervision" class="btn btn-primary">Ir Carga Masiva</a></p>
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaDicapiDireccionInspeccionAuditoriaSupervision()" />
   <div class="row align-items-start">
        <div class="col"><select class="form-select form-control-sm" id="cargas" name="cargas"></select></div>
        <div class="col">
            <input type='button' id="btn_search" value="Buscar" class="btn btn-primary">
            <input type='button' id="btn_all" value="Mostrar Todos" class="btn btn-primary">
        </div>
    </div>
    <table class="table table-striped" id="tblDicapiDireccionInspeccionAuditoriaSupervision" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;overflow:scroll">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>N° Nombramiento</th>
                <th>Fecha de Inspeccion</th>
                <th>Capitania</th>
                <th>1° Inspector</th>
                <th>2° Inspector</th>
                <th>Código Carga</th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

@section Scripts{
    <script src="/js/dicapi/dicapi-direccioninspeccionauditoriasupervision.js" asp-append-version="true"></script>
}
