@{
    ViewData["Title"] = "Situación de Operatividad de Aeronaves";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1><a href="~/ComescuamaSituacionOperatividadAeronave/Individual" style="text-decoration:none;">Situación de Operatividad de Aeronaves</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="CategoriaAeronave" class="col-3 col-form-label"><a href="~/CategoriaAeronave">AeroNave</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbCategoriaAeronave" name="cbCategoriaAeronave"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NumeroCola" class="col-3 col-form-label">N° de Cola</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtNumeroCola" name="txtNumeroCola" placeholder="N° de Cola" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="TipoPlataformaAeronave" class="col-3 col-form-label"><a href="~/TipoPlataformaAeronave">Tipo de Plataforma</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbTipoPlataformaAeronave" name="cbTipoPlataformaAeronave"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Dependencia" class="col-3 col-form-label"><a href="~/Dependencia">Dependencia</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbDependencia" name="cbDependencia"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Ubicacion" class="col-3 col-form-label">Ubicación</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtUbicacion" name="txtUbicacion" placeholder="Ubicación" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="DepartamentoUbigeo" class="col-3 col-form-label"><a href="~/DepartamentoUbigeo">Departamento</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbDepartamentoUbigeo" name="cbDepartamentoUbigeo"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ProvinciaUbigeo" class="col-3 col-form-label"><a href="~/ProvinciaUbigeo">Provincia</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbProvinciaUbigeo" name="cbProvinciaUbigeo"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="DistritoUbigeo" class="col-3 col-form-label"><a href="~/DistritoUbigeo">Distrito</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbDistritoUbigeo" name="cbDistritoUbigeo"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="CapacidadOperativaRequerida" class="col-3 col-form-label"><a href="~/CapacidadOperativaRequerida">Capacidad Operativa Requerida</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbCapacidadOperativaRequerida" name="cbCapacidadOperativaRequerida"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Condicion" class="col-3 col-form-label"><a href="~/Condicion">Condición</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbCondicion" name="cbCondicion"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Observacion" class="col-3 col-form-label">Observaciones</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtObservacion" name="txtObservacion" placeholder="Observaciones" required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/ComescuamaSituacionOperatividadAeronave">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
