<div id="listar" style="overflow:scroll">
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaComescuamaSituacionOperatividadAeronave()" />
    <div class="dropdown">
        <button class="dropdown-toggle" type="button" id="dropdownMenuButton" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            Modos de Carga :
        </button>
        <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
            <a class="dropdown-item" href="~/ComescuamaSituacionOperatividadAeronave">Masiva</a>
            <a class="dropdown-item" href="~/ComescuamaSituacionOperatividadAeronave/Individual">Individual</a>
        </div>
    </div>
    <table class="table table-striped" id="tblComescuamaSituacionOperatividadAeronave" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;overflow:scroll">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>AeroNave</th>
                <th>N° de Cola</th>
                <th>Tipo de Plataforma</th>
                <th>Dependencia</th>
                <th>Ubicación</th>
                <th>Departamento</th>
                <th>Provincia</th>
                <th>Distrito</th>
                <th>Capacidad Operativa Requerida</th>
                <th>Condición</th>
                <th>Observaciones</th>
                <th colspan="2" class="text-center">Acciones</th>
