@{
    ViewData["Title"] = "Docentes Diresna";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1><a href="~/DiresnaDocenteEsna/Individual" style="text-decoration:none;">Docentes Diresna</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <select class="form-select form-control-sm" id="cargasR" name="cargasR"></select>
                </div>
                <div class="col-sm-2">
                    <input type="date" class="form-control form-control-sm" id="txtFecha" name="txtFecha" required>
                </div>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="DNI" class="col-3 col-form-label">DNI</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtDNI" name="txtDNI" placeholder="DNI" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="TipoPersonalD" class="col-3 col-form-label">Tipo de Personal Docente</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtTipoPersonalD" name="txtTipoPersonalD" placeholder="Tipo de Personal Docente" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="CondicionLaboralD" class="col-3 col-form-label"><a href="~/CondicionLaboralDocente">Condición Laboral del Docente</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbCondicionLaboralD" name="cbCondicionLaboralD"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Regimenlaboral " class="col-3 col-form-label"><a href="~/RegimenLaboral">Régimen laboral </a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbRegimenlaboral" name="cbRegimenlaboral"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="DedicacionDocente" class="col-3 col-form-label">Dedicación del Docente</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtDedicacionDocente" name="txtDedicacionDocente" placeholder="Dedicación del Docente" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NivelEstudios" class="col-3 col-form-label"><a href="~/NivelEstudio">Nivel Estudios</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbNivelEstudios" name="cbNivelEstudios"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="CarreraUniversitariaEspe" class="col-3 col-form-label"><a href="~/CarreraUniversitariaEspecialidad">Carreras de Educación Superior</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbCarreraUniversitariaEspe" name="cbCarreraUniversitariaEspe"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ExperienciaDocente" class="col-3 col-form-label">Experiencia Laboral en Docencia (años)</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtExperienciaDocente" name="txtExperienciaDocente" placeholder="Experiencia Laboral en Docencia (años)" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ExperienciaDocenteMarina" class="col-3 col-form-label">Experiencia laboral en docencia en marina (años)</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtExperienciaDocenteMarina" name="txtExperienciaDocenteMarina" placeholder="Experiencia laboral en docencia en marina (años)" required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DiresnaDocenteEsna">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="editar" style="display:none;">
    <form class="form-actualizacion" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <input type="hidden" id="txtCodigo" />
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Editar</h4>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="DNI" class="col-3 col-form-label">DNI</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtDNIe" name="txtDNIe" placeholder="DNI" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="TipoPersonalD" class="col-3 col-form-label">Tipo de Personal Docente</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtTipoPersonalDe" name="txtTipoPersonalDe" placeholder="Tipo de Personal Docente" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="CondicionLaboralD" class="col-3 col-form-label"><a href="~/CondicionLaboralDocente">Condición Laboral del Docente</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbCondicionLaboralDe" name="cbCondicionLaboralDe"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Regimenlaboral " class="col-3 col-form-label"><a href="~/RegimenLaboral">Régimen laboral </a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbRegimenlaborale" name="cbRegimenlaborale"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="DedicacionDocente" class="col-3 col-form-label">Dedicación del Docente</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtDedicacionDocentee" name="txtDedicacionDocentee" placeholder="Dedicación del Docente" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NivelEstudios" class="col-3 col-form-label"><a href="~/NivelEstudio">Nivel Estudios</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbNivelEstudiose" name="cbNivelEstudiose"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="CarreraUniversitariaEspe" class="col-3 col-form-label"><a href="~/CarreraUniversitariaEspecialidad">Carreras de Educación Superior</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbCarreraUniversitariaEspee" name="cbCarreraUniversitariaEspee"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ExperienciaDocente" class="col-3 col-form-label">Experiencia Laboral en Docencia (años)</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtExperienciaDocentee" name="txtExperienciaDocentee" placeholder="Experiencia Laboral en Docencia (años)" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ExperienciaDocenteMarina" class="col-3 col-form-label">Experiencia laboral en docencia en marina (años)</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtExperienciaDocenteMarinae" name="txtExperienciaDocenteMarinae" placeholder="Experiencia laboral en docencia en marina (años)" required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DiresnaDocenteEsna">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="listar" style="overflow:scroll">
    <p><a href="~/DiresnaDocenteEsna" class="btn btn-primary">Ir Carga Masiva</a></p>
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaDiresnaDocenteEsna()" />
    <div class="row align-items-start">
        <div class="col">
            <select class="form-select form-control-sm" id="cargas" name="cargas"></select>
        </div>
        <div class="col">
            <button class="btn btn-primary" id="btn_search">Buscar</button>
            <button class="btn btn-primary" id="btn_all">Mostrar Todos</button>
            <button class="btn btn-danger" onclick="eliminarCarga()">Eliminar Carga</button>
        </div>
    </div>
    <table class="table table-striped" id="tblDiresnaDocenteEsna" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;overflow:scroll">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>DNI</th>
                <th>Tipo de Personal Docente</th>
                <th>Condición Laboral del Docente</th>
                <th>Régimen laboral </th>
                <th>Dedicación del Docente</th>
                <th>Nivel Estudios</th>
                <th>Carreras de Educación Superior</th>
                <th>Experiencia Laboral en Docencia (años)</th>
                <th>Experiencia laboral en docencia en marina (años)</th>
                <th>Codigo Carga</th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

@section Scripts{
    <script src="/js/diresna/diresna-docenteesna.js" asp-append-version="true"></script>
}
