@{
    ViewData["Title"] = "Formato";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-sm-12">
        <h1>
            <a href="~/ComfuavinavAlistamientoRepuestoCritico" style="text-decoration:none;">
                Alistamiento de repuestos críticos (ARC)
            </a>
        </h1>
        <div class="card">
            <div class="card-header">
                <div class="card-header">
                    <a href="~/ComfuavinavAlistamientoRepuestoCritico/Individual" class="btn btn-primary">Ir Carga Individual</a>
                </div>
            </div>
            <div class="card-body">

                <div class="row">
                    <div class="col-sm-4">
                        <input class="form-control-file" type="file" id="inputExcel" />
                    </div>
                    <div class="col-sm-4">
                        <button class="btn btn-warning" onclick="mostrarDatos()">Mostrar Datos"</button>
                        <button class="btn btn-success" onclick="enviarDatos()">Enviar Datos a BD</button>
                        <label>Numero de Registros : </label><span id="NumRegistros"></span>
                    </div>
                </div>
                <hr />
                <div class="row" style="overflow:scroll">
                    <div class="col-sm-12">
                        <table id="tbData" class="table table-striped">
                            <thead>
                                <tr>
                                    <th>Código</th>
                                    <th>Unidad</th>
                                    <th>Sistema</th>
                                    <th>Subsistema</th>
                                    <th>Equipo</th>
                                    <th>
                                        Alistamiento de repuestos(descripción) listados de logística inicial requerida (LIR) por unidad tipo
                                    </th>
                                    <th>N° de repuestos existentes</th>
                                    <th>
                                        N° de repuestos necesarios
                                    </th>
                                    <th>
                                        Coeficiente de podendaración para repuestos según la unidad y sistema
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
        </div>

    </div>
</div>
@section Scripts{
    <script src="/js/comfuavinav/comfuavinav-alistamientorepuestocritico.js" asp-append-version="true"></script>
}