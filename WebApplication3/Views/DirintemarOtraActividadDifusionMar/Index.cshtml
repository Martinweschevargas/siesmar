@{
    ViewData["Title"] = "Formato";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-sm-12">
        <h1><a href="~/DirintemarOtraActividadDifusionMar" style="text-decoration:none;">Otra Actividades de Realidad de Difusión e Interés Maritimo</a></h1>
        <div class="card">
            <div class="card-header">
                <div class="card-header">
                    <a href="~/DirintemarOtraActividadDifusionMar/Individual" class="btn btn-primary">Ir Carga Individual</a>
                    <a href="~/DirintemarOtraActividadDifusionMar/DownloadFile" class="btn btn-primary">Descargar Formato</a>
                </div>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-sm-4">
                        <input class="form-control" type="file" id="inputExcel" />
                    </div>
                    <div class="col-sm-4">
                        <button class="btn btn-warning" onclick="mostrarDatos()">Vista Previa</button>
                        <button class="btn btn-success" onclick="enviarDatos()">Guardar Formato</button>
                    </div>
                    <div class="col-sm-4">
                        <label>Numero de Registros : </label><span id="NumRegistros"></span>
                    </div>
                </div>
                <hr />
                <div class="row" style="overflow:scroll">
                    <div class="col-sm-12">
                        <table class="table table-striped" id="tbData" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;">
                            <thead>
                                <tr class="colorbg">
                                    <th>Código</th>
                                    <th>Tipo Actividad Difusión</th>
                                    <th>Nombre Otra Actividad</th>
                                    <th>Área</th>
                                    <th>Responsable</th>
                                    <th>Fecha Inicio</th>
                                    <th>Fecha Término</th>
                                    <th>Lugar</th>
                                    <th>Dirigido</th>
                                    <th>Participantes</th>
                                    <th>Participantes Encuesta</th>
                                    <th>Participantes Encuesta OBS</th>
                                    <th>Respuesta Correcta</th>
                                    <th>Respuesta Incorrecta</th>
                                    <th>Porcentaje Respuetas Correctas</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
        </div>

    </div>
</div>
@section Scripts{
    <script src="/js/dirintemar/dirintemar-actividadcultural.js" asp-append-version="true"></script>
}