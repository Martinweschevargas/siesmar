@{
    ViewData["Title"] = "Exp Documento Personal Acuatico";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1><a href="~/DicapiExpDocumentoPersonalAcuatico/Individual" style="text-decoration:none;">Expedición de Documentos al Personal Acuatico</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>

            <div class="form-group row col-md-6">
                <label for="carga" class="col-3 col-form-label">Carga</label>
                <div class="form-group row col-md-6">
                    <div class="col"><select class="form-select-sm form-control-sm" id="cargasR" name="cargasR"></select></div>
                </div>
            </div>

            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="Numero" class="col-3 col-form-label">N° Documento</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtNumero" name="txtNumero" placeholder="N° Documento" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaIngreso" class="col-3 col-form-label">Fecha de Ingreso Solicitud</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaIngreso" name="txtFechaIngreso" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="DptoAcuatico" class="col-3 col-form-label"><a href="~/DptoPersonalAcuatico">Codigo Procedimiento TUPAM</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbDptoAcuatico" name="cbDptoAcuatico"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Documento" class="col-3 col-form-label">Documento Expedido</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtDocumento" name="txtDocumento" placeholder="Documento Expedido" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Expedido" class="col-3 col-form-label">Se Expide a:</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtExpedido" name="txtExpedido" placeholder="Se Expide a:" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NombreApellido" class="col-3 col-form-label">Nombre y Apellido de Personal Acuatico / Nombre de la empresa</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtNombreApellido" name="txtNombreApellido" placeholder="Nombre y Apellido de Personal Acuatico / Nombre de la empresa" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Personal" class="col-3 col-form-label"><a href="~/TipoPersonalAcuatico">Clasificacion de Personal Acuatico</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbPersonal" name="cbPersonal"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Actividad" class="col-3 col-form-label"><a href="~/TipoActividadEmpresa">Actividad de la Empresa</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbActividad" name="cbActividad"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaAtencion" class="col-3 col-form-label">Fecha de la Atencion Solicitud</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaAtencion" name="txtFechaAtencion" required>
                    </div>
                </div>
            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DicapiExpDocumentoPersonalAcuatico">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="editar" style="display:none;">
    <form class="form-actualizacion" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <input type="hidden" id="txtCodigo" />
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Editar</h4>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="Numero" class="col-3 col-form-label">N° Documento</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtNumeroe" name="txtNumeroe" placeholder="N° Documento" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaIngreso" class="col-3 col-form-label">Fecha de Ingreso Solicitud</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaIngresoe" name="txtFechaIngresoe" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="DptoAcuatico" class="col-3 col-form-label"><a href="~/DptoPersonalAcuatico">Codigo Procedimiento TUPAM</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbDptoAcuaticoe" name="cbDptoAcuaticoe"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Documento" class="col-3 col-form-label">Documento Expedido</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtDocumentoe" name="txtDocumentoe" placeholder="Documento Expedido" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Expedido" class="col-3 col-form-label">Se Expide a:</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtExpedidoe" name="txtExpedidoe" placeholder="Se Expide a:" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NombreApellido" class="col-3 col-form-label">Nombre y Apellido de Personal Acuatico / Nombre de la empresa</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtNombreApellidoe" name="txtNombreApellidoe" placeholder="Nombre y Apellido de Personal Acuatico / Nombre de la empresa" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Personal" class="col-3 col-form-label"><a href="~/TipoPersonalAcuatico">Clasificacion de Personal Acuatico</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbPersonale" name="cbPersonale"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Actividad" class="col-3 col-form-label"><a href="~/TipoActividadEmpresa">Actividad de la Empresa</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbActividade" name="cbActividade"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaAtencion" class="col-3 col-form-label">Fecha de la Atencion Solicitud</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaAtencione" name="txtFechaAtencione" required>
                    </div>
                </div>
                <div class="row mt-2">
                    <div class="col-md-6 text-center">
                        <button type="submit" class="btn btn-primary">Grabar</button>
                    </div>
                    <div class="col-md-6 text-center">
                        <a class="btn btn-danger" href="~/DicapiExpDocumentoPersonalAcuatico">Cancelar</a>
                    </div>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="listar" style="overflow:scroll">
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaDicapiExpDocumentoPersonalAcuatico()" />
    <div class="dropdown">
        <button class="dropdown-toggle" type="button" id="dropdownMenuButton" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            Modos de Carga :
        </button>
        <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
            <a class="dropdown-item" href="~/DicapiExpDocumentoPersonalAcuatico">Masiva</a>
            <a class="dropdown-item" href="~/DicapiExpDocumentoPersonalAcuatico/Individual">Individual</a>
        </div>
    </div>
    <table class="table table-striped" id="tblDicapiExpDocumentoPersonalAcuatico" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;overflow:scroll">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>N° Documento</th>
                <th>Fecha de Ingreso Solicitud</th>
                <th>Codigo Procedimiento TUPAM</th>
                <th>Documento Expedido</th>
                <th>Se Expide a:</th>
                <th>Nombre y Apellido de Personal Acuatico / Nombre de la empresa</th>
                <th>Clasificacion de Personal Acuatico</th>
                <th>Actividad de la Empresa</th>
                <th>Fecha de la Atencion Solicitud</th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

@section Scripts{
    <script src="/js/dicapi/dicapi-expdocumentopersonalacuatico.js" asp-append-version="true"></script>
}
