                                    <th>CIE103</th>
                                    <th>Diagnóstico o motivo de atención4</th>
                                    <th>Tipo de dx4</th>
                                    <th>CIE104</th>
                                    <th>Diagnóstico o motivo de atención5</th>
                                    <th>Tipo de dx5</th>
                                    <th>CIE105</th>
                                    <th>Diagnóstico o motivo de atención6</th>
                                    <th>Tipo de dx6</th>
                                    <th>CIE106</th>
                                    <th>Tipo de emergencia</th>
                                    <th>Interconsulta</th>
                                    <th>Especialidad de interconsulta</th>
                                    <th>motivo de la emergencia</th>
                                    <th>acompañante</th>
                                    <th>Dni</th>
                                    <th>Condición de alta</th>
                                    <th>Destino posterior</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
        </div>

    </div>
</div>
@section Scripts{
    <script src="/js/disamar/disamar-registroatencionemergencia.js" asp-append-version="true"></script>
}