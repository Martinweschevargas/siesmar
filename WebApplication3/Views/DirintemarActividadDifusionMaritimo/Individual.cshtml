<div id="listar" style="overflow:scroll">
    <p><a href="~/DirintemarActividadDifusionMaritimo" class="btn btn-primary">Ir Carga Masiva</a></p>
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaDirintemarActividadDifusionMaritimo()" />
    <div class="dropdown">
        <input type='text' readonly id='buscar_inicio' class="datepicker" placeholder='Fecha Inicio' class="form-control form-control-sm">
        <input type='text' readonly id='buscar_fin' class="datepicker" placeholder='Fecha fin' class="form-control form-control-sm">
        <input type='button' id="btn_search" value="Buscar" class="btn btn-primary btn-sm">
        @*<input type='button' id="btnLimpiar" value="Limpiar" class="btn btn-danger btn-sm">*@
    </div>
    <table class="table table-striped" id="tblDirintemarActividadDifusionMaritimo" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>Tipo Actividad Difusión </th>
                <th>Nombre Actividad </th>
                <th>Área </th>
                <th>Responsable </th>
                <th>Fecha Inicio </th>
                <th>Fecha Término</th>
                <th>Lugar </th>
                <th>Participantes</th>
                <th>Participantes Encuesta</th>
                <th>Preguntas Encuesta OBS</th>
                <th>Respuesta Correcta</th>
                <th>Respuesta Incorrecta</th>
                <th>Porcentaje Respuestas Correctas</th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
