@{
    ViewData["Title"] = "DocentesDiresna";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1><a href="~/DiresnaEgresoPromocionesDiresna/Individual" style="text-decoration:none;">Egreso por Promociones Diresna</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <select class="form-select form-control-sm" id="cargasR" name="cargasR"></select>
                </div>
                <div class="col-sm-2">
                    <input type="date" class="form-control form-control-sm" id="txtFecha" name="txtFecha" required>
                </div>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="DNI" class="col-3 col-form-label">DNI</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtDNI" name="txtDNI" placeholder="DNI" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Genero" class="col-3 col-form-label">Género</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtGenero" name="txtGenero" placeholder="Género" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaResolI" class="col-3 col-form-label">Fecha de Resolución de Ingreso</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaResolI" name="txtFechaResolI" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaResolE" class="col-3 col-form-label">Fecha de Resolución de Egreso</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaResolE" name="txtFechaResolE" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ConcursoAdmisionI" class="col-3 col-form-label">Concurso Admisión Ingreso</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtConcursoAdmisionI" name="txtConcursoAdmisionI" placeholder="Concurso Admisión Ingreso" required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DiresnaEgresoPromocionesDiresna">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="editar" style="display:none;">
    <form class="form-actualizacion" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <input type="hidden" id="txtCodigo" />
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Editar</h4>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="DNI" class="col-3 col-form-label">DNI</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtDNIe" name="txtDNIe" placeholder="DNI" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Genero" class="col-3 col-form-label">Género</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtGeneroe" name="txtGeneroe" placeholder="Género" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaResolI" class="col-3 col-form-label">Fecha de Resolución de Ingreso</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaResolIe" name="txtFechaResolIe" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaResolE" class="col-3 col-form-label">Fecha de Resolución de Egreso</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaResolEe" name="txtFechaResolEe" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ConcursoAdmisionI" class="col-3 col-form-label">Concurso Admisión Ingreso</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtConcursoAdmisionIe" name="txtConcursoAdmisionIe" placeholder="Concurso Admisión Ingreso" required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DiresnaEgresoPromocionesDiresna">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="listar" style="overflow:scroll">
    <p><a href="~/DiresnaEgresoPromocionesDiresna" class="btn btn-primary">Ir Carga Masiva</a></p>
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaDiresnaEgresoPromocionesDiresna()" />
    <div class="row align-items-start">
        <div class="col">
            <select class="form-select form-control-sm" id="cargas" name="cargas"></select>
        </div>
        <div class="col">
            <button class="btn btn-primary" id="btn_search">Buscar</button>
            <button class="btn btn-primary" id="btn_all">Mostrar Todos</button>
            <button class="btn btn-danger" onclick="eliminarCarga()">Eliminar Carga</button>
        </div>
    </div>
    <table class="table table-striped" id="tblDiresnaEgresoPromocionesDiresna" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;overflow:scroll">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>DNI</th>
                <th>Sexo</th>
                <th>Fecha de Resolución de Ingreso</th>
                <th>Fecha de Resolución de Egreso</th>
                <th>Concurso Admisión Ingreso</th>
                <th>Codigo Carga</th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

@section Scripts{
    <script src="/js/diresna/diresna-egresopromocionesdiresna.js" asp-append-version="true"></script>
}
