@{
    ViewData["Title"] = "Formato";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-sm-12">
        <h1>
            <a href="~/Comgoe3AlistamientoCombustibleLubricanteComgoe" style="text-decoration:none;">
                Alistamiento de combustibles y lubricantes (ACL)
            </a>
        </h1>
        <div class="card">
            <div class="card-header">
                <div class="card-header">
                    <a href="~/Comgoe3AlistamientoCombustibleLubricanteComgoe/Individual" class="btn btn-primary">Ir Carga Individual</a>
                    <a href="~/Comgoe3AlistamientoCombustibleLubricanteComgoe/DownloadFile" class="btn btn-primary">Descargar Formato</a>
                </div>
            </div>
            <div class="card-body">

                <div class="row">
                    <div class="col-sm-4">
                        <input class="form-control-file" type="file" id="inputExcel" />
                    </div>
                    <div class="col-sm-4">
                        <button class="btn btn-warning" onclick="mostrarDatos()">Mostrar Datos"</button>
                        <button class="btn btn-success" onclick="enviarDatos()">Enviar Datos a BD</button>
                        <label>Numero de Registros : </label><span id="NumRegistros"></span>
                    </div>
                </div>
                <hr />
                <div class="row" style="overflow:scroll">
                    <div class="col-sm-12">
                        <table id="tbData" class="table table-striped">
                            <thead>
                                <tr>
                                    <th>Código</th>
                                    <th>Unidad</th>
                                    <th>Artículo</th>
                                    <th>Equipo</th>
                                    <th>Unidad de medida</th>
                                    <th>Cargo</th>
                                    <th>Aumento</th>
                                    <th>Consumo</th>
                                    <th>Existencia</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
        </div>

    </div>
</div>
@section Scripts{
    <script src="/js/comgoe3/comgoe3-alistamientocombustiblelubricantecomgoe.js" asp-append-version="true"></script>
}