@{
    ViewData["Title"] = "Formatos Para Ingreso Datos Dirección Capacitación Personal Naval";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1><a href="~/DircapenIngresoDatosCapacitacionNaval/Individual" style="text-decoration:none;">Formatos para el Ingreso de Datos de la Dirección de Capacitación del Personal Naval</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <select class="form-select form-control-sm" id="cargasR" name="cargasR"></select>
                </div>
                <div class="col-sm-2">
                    <input type="date" class="form-control form-control-sm" id="txtFecha" name="txtFecha" required>
                </div>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="CIP" class="col-3 col-form-label">CIP</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtCIP" name="txtCIP" placeholder="CIP" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="DNI" class="col-3 col-form-label">DNI</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtDNI" name="txtDNI" placeholder="DNI" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Sexo" class="col-3 col-form-label">Sexo</label>
                    <div class="col-md-9">                  
                        <select class="form-select form-select-sm" id="txtSexo" name="txtSexo" data-placeholder="Seleccione ...">
                            <option value="M">Masculino</option>
                            <option value="F">Femenino</option>
                        </select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="TipoPersonal" class="col-3 col-form-label"><a href="~/TipoPersonalMilitar" style="text-decoration:none;">Tipo del Personal</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbTipoPersonal" name="cbTipoPersonal"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Grado" class="col-3 col-form-label"><a href="~/GradoPersonalMilitar" style="text-decoration:none;">Grado</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbGrado" name="cbGrado"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="EspecialidadGenerica" class="col-3 col-form-label"><a href="~/EspecialidadGenericaPersonal" style="text-decoration:none;">Especialidad Genérica</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbEspecialidadGenerica" name="cbEspecialidadGenerica"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Dependencia" class="col-3 col-form-label"><a href="~/Dependencia" style="text-decoration:none;">Dependencia</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbDependencia" name="cbDependencia"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ZonaNaval" class="col-3 col-form-label"><a href="~/ZonaNaval" style="text-decoration:none;">Zona Naval</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbZonaNaval" name="cbZonaNaval"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Nivel" class="col-3 col-form-label"><a href="~/ProgramaEspecializacionEspecifico" style="text-decoration:none;">Programa Especializacion</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbNivel" name="cbNivel"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Programa" class="col-3 col-form-label"><a href="~/ProgramaEspecializacionGrupo" style="text-decoration:none;">Especializacion Grupo</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbPrograma" name="cbPrograma"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaInicio" class="col-3 col-form-label">FechaInicio</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaInicio" name="txtFechaInicio" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaTermino" class="col-3 col-form-label">FechaTérmino</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaTermino" name="txtFechaTermino" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Modalidad" class="col-3 col-form-label"><a href="~/TipoModalidad" style="text-decoration:none;">Modalidad</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbModalidad" name="cbModalidad"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ConcluyoPrograma" class="col-3 col-form-label">Concluyó Programa de Estudios</label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="txtConcluyoPrograma" name="txtConcluyoPrograma" data-placeholder="Seleccione ...">
                            <option value="S">Si</option>
                            <option value="N">No</option>
                        </select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="TotalCreditos" class="col-3 col-form-label">Total Créditos</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtTotalCreditos" name="txtTotalCreditos" placeholder="Total Créditos" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="MotivosNoConcluir" class="col-3 col-form-label">Motivos de no Concluir</label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="txtMotivosNoConcluir" name="txtMotivosNoConcluir" data-placeholder="Seleccione ...">
                            <option value="1">Fallecimiento</option>
                            <option value="2">Alto Índice de inasistencia más 30%</option>
                            <option value="3">Negarse a rendir un examen o practica)</option>
                            <option value="4">Por cometer delitos o faltas sancionadas con rigor)</option>
                            <option value="5">Mala conducta habitual</option>
                            <option value="6">A su solicitud siempre y cuando el programa no tenga una duración mayor a 45 días</option>
                            <option value="7">Por razones del servicio)</option>
                            <option value="8">Por falta de destreza y/o actitud en la fase practica o exposición</option>
                        </select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="CalificacionFinal" class="col-3 col-form-label">Calificación Final Obtenida</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtCalificacionFinal" name="txtCalificacionFinal" placeholder="Calificación Final Obtenida" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Diploma" class="col-3 col-form-label">Diploma</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtDiploma" name="txtDiploma" placeholder="Diploma" required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DircapenIngresoDatosCapacitacionNaval">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
