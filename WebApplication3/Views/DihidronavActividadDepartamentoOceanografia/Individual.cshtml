@{
    ViewData["Title"] = "Actividad Departamento Oceanografia";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1><a href="~/DihidronavActividadDepartamentoOceanografia/Individual" style="text-decoration:none;">Actividades del Departamento de Oceanografia</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>
            <div class="form-group row col-md-6">
                <label for="carga" class="col-3 col-form-label">Carga</label>
                <div class="form-group row col-md-6">
                    <div class="col"><select class="form-select-sm form-control-sm" id="cargasR" name="cargasR"></select></div>
                </div>
            </div>

            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="Orden" class="col-3 col-form-label">N° Orden</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtOrden" name="txtOrden" placeholder="N° Orden" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Oceanografico" class="col-3 col-form-label"><a href="~/TrabajoOceanografico">Tipo de Trabajo Oceanografico</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbOceanografico" name="cbOceanografico"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Efectuado" class="col-3 col-form-label">Descripcion del Trabajo Efectuado</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtEfectuado" name="txtEfectuado" placeholder="Descripcion del Trabajo Efectuado" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Zona" class="col-3 col-form-label"><a href="~/ZonaNautica">Zona</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbZona" name="cbZona"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Departamento" class="col-3 col-form-label"><a href="~/DepartamentoUbigeo">Departamento</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbDepartamento" name="cbDepartamento"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Provincia" class="col-3 col-form-label"><a href="~/ProvinciaUbigeo">Provincia</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbProvincia" name="cbProvincia"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Distrito" class="col-3 col-form-label"><a href="~/DistritoUbigeo">Distrito</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbDistrito" name="cbDistrito"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaIni" class="col-3 col-form-label">Fecha de Inicio</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaIni" name="txtFechaIni" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaTer" class="col-3 col-form-label">Fecha de Termino</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaTer" name="txtFechaTer" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Situacion" class="col-3 col-form-label">Situacion del Trabajo Efectuado</label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="txtSituacion" name="txtSituacion" data-placeholder="Seleccione ...">
                            <option value="En proyecto">En proyecto</option>
                            <option value="En ejecucion">En ejecucion</option>
                            <option value="Concluido">Concluido</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DihidronavActividadDepartamentoOceanografia">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="editar" style="display:none;">
    <form class="form-actualizacion" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <input type="hidden" id="txtCodigo" />
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Editar</h4>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="Orden" class="col-3 col-form-label">N° Orden</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtOrdene" name="txtOrdene" placeholder="N° Orden" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Oceanografico" class="col-3 col-form-label"><a href="~/TrabajoOceanografico">Tipo de Trabajo Oceanografico</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbOceanograficoe" name="cbOceanograficoe"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Efectuado" class="col-3 col-form-label">Descripcion del Trabajo Efectuado</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtEfectuadoe" name="txtEfectuadoe" placeholder="Descripcion del Trabajo Efectuado" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Zona" class="col-3 col-form-label"><a href="~/ZonaNautica">Zona</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbZonae" name="cbZonae"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Departamento" class="col-3 col-form-label"><a href="~/DepartamentoUbigeo">Departamento</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbDepartamentoe" name="cbDepartamentoe"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Provincia" class="col-3 col-form-label"><a href="~/ProvinciaUbigeo">Provincia</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbProvinciae" name="cbProvinciae"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Distrito" class="col-3 col-form-label"><a href="~/DistritoUbigeo">Distrito</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbDistritoe" name="cbDistritoe"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaIni" class="col-3 col-form-label">Fecha de Inicio</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaInie" name="txtFechaInie" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaTer" class="col-3 col-form-label">Fecha de Termino</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaTere" name="txtFechaTere" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Situacion" class="col-3 col-form-label">Situacion del Trabajo Efectuado</label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="txtSituacione" name="txtSituacione" data-placeholder="Seleccione ...">
                            <option value="En proyecto">En proyecto</option>
                            <option value="En ejecucion">En ejecucion</option>
                            <option value="Concluido">Concluido</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DihidronavActividadDepartamentoOceanografia">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="listar" style="overflow:scroll">
    <p><a href="~/DihidronavActividadDepartamentoOceanografia" class="btn btn-primary">Ir Carga Masiva</a></p>
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaDihidronavActividadDepartamentoOceanografia()" />
    <div class="row align-items-start">
        <div class="col"><select class="form-select form-control-sm" id="cargas" name="cargas"></select></div>
        <div class="col">
            <input type='button' id="btn_search" value="Buscar" class="btn btn-primary">
            <input type='button' id="btn_all" value="Mostrar Todos" class="btn btn-primary">
        </div>

    </div>

    <table class="table table-striped" id="tblDihidronavActividadDepartamentoOceanografia" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;overflow:scroll">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>N° Orden</th>
                <th>Tipo de Trabajo Oceanografico</th>
                <th>Descripcion del Trabajo Efectuado</th>
                <th>Zona</th>
                <th>Departamento</th>
                <th>Provincia</th>
                <th>Distrito</th>
                <th>Fecha de Inicio</th>
                <th>Fecha de Termino</th>
                <th>Situacion del Trabajo Efectuado</th>
                <th>Código Cargo</th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

@section Scripts{
    <script src="/js/dihidronav/dihidronav-actividaddepartamentooceanografia.js" asp-append-version="true"></script>
}
