@{
    ViewData["Title"] = "Formato";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-sm-12">
        <h1><a href="~/DirnotematProcesoInternamiento" style="text-decoration:none;">Proceso de Internamiento </a></h1>
        <div class="card">
            <div class="card-header">
                <div class="card-header">
                    <a href="~/DirnotematProcesoInternamiento/Individual" class="btn btn-primary">Ir Carga Individual</a>
                    <a href="~/DirnotematProcesoInternamiento/DownloadFile" class="btn btn-primary">Descargar Formato</a>
                    <a class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#ventanaModal">Reportes</a>
                </div>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="row">
                        <div class="col-sm-4">
                            <input class="form-control form-control-sm" type="file" id="inputExcel" />
                        </div>
                        <div class="col-sm-2">
                            <input type="date" class="form-control form-control-sm" id="txtFecha" name="txtFecha" required>
                        </div>
                    </div>
                    <div class="row mt-4">
                        <div class="col-sm-6 text-center">
                            <button class="btn btn-warning" onclick="mostrarDatos()">Previsualizar Datos</button>
                        </div>
                        <div class="col-sm-6 text-center">
                            <button class="btn btn-success" onclick="enviarDatos()">Grabar Formato</button>
                        </div>
                    </div>
                </div>
                <hr />
                <div class="row" style="overflow:scroll">
                    <div class="col-sm-12">
                        <table class="table table-striped" id="tbData" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;">
                            <thead>
                                <tr class="colorbg">
                                    <th>Nombre Proceso</th>
                                    <th>Nro Contrato</th>
                                    <th>Tipo Proceso </th>
                                    <th>Nro Proceso Internamiento</th>
                                    <th>Nro Guia Proceso</th>
                                    <th>Fecha Ingreso </th>
                                    <th>Tiempo Evaluación</th>
                                    <th>Resultado Evaluación</th>
                                    <th>Laboratorio CT </th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
          
        </div>

    </div>
</div>

@section Scripts{
    <script src="/js/dirnotemat/dirnotemat-procesointernaniento.js" asp-append-version="true"></script>
}