@{
    ViewData["Title"] = "Evaluación del Alistamiento de Dotación de Vuelo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1><a href="~/ComfuavinavEvaluacionAlistamientoDotacionVuelo/Individual" style="text-decoration:none;">Evaluación del Alistamiento de Dotación de Vuelo</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <select class="form-select form-control-sm" id="cargasR" name="cargasR"></select>
                </div>
                <div class="col-sm-2">
                    <input type="date" class="form-control form-control-sm" id="txtFecha" name="txtFecha" required>
                </div>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="UnidadNaval" class="col-3 col-form-label"><a href="~/UnidadNaval">Unidad</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbUnidadNaval" name="cbUnidadNaval"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaEvaluacion" class="col-3 col-form-label">Fecha</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaEvaluacion" name="txtFechaEvaluacion" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="DNIPersonal" class="col-3 col-form-label">DNI</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtDNIPersonal" name="txtDNIPersonal" placeholder="DNI" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="CIPPersonal" class="col-3 col-form-label">CIP</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtCIPPersonal" name="txtCIPPersonal" placeholder="CIP" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="CargoPersonal" class="col-3 col-form-label"><a href="~/Cargo">Cargo Personal</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbCargoPersonal" name="cbCargoPersonal"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="GradoPersonalMilitarEsperado" class="col-3 col-form-label"><a href="~/GradoPersonalMilitar">Grado-Esperado</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbGradoPersonalMilitarEsperado" name="cbGradoPersonalMilitarEsperado"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="EspecialidadGenericaEsperado" class="col-3 col-form-label"><a href="~/EspecialidadGenericaPersonal">Especialidad-Esperado</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbEspecialidadGenericaEsperado" name="cbEspecialidadGenericaEsperado"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="GradoPersonalMilitarActual" class="col-3 col-form-label"><a href="~/GradoPersonalMilitar">Grado-Actual</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbGradoPersonalMilitarActual" name="cbGradoPersonalMilitarActual"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="EspecialidadGenericaActual" class="col-3 col-form-label"><a href="~/EspecialidadGenericaPersonal">Especialidad-Actual</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbEspecialidadGenericaActual" name="cbEspecialidadGenericaActual"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="GradoJerarquico" class="col-3 col-form-label">Grado Jerárquico</label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="txtGradoJerarquico" name="txtGradoJerarquico" data-placeholder="Seleccione ...">
                            <option value="1">Posee grado adecuado</option>
                            <option value="0.75">No posee grado adecuado</option>
                        </select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ServicioExperiencia" class="col-3 col-form-label">Servicio o Experiencia</label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="txtServicioExperiencia" name="txtServicioExperiencia" data-placeholder="Seleccione ...">
                            <option value="1">Experiencia adecuada</option>
                            <option value="0.75">Sin experiencia adecuada</option>
                        </select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="EspecializacionProfesional" class="col-3 col-form-label">Especialización Profesional Técnica</label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="txtEspecializacionProfesional" name="txtEspecializacionProfesional" data-placeholder="Seleccione ...">
                            <option value="1">Cuenta con especializaciones profesionales/especializaciones técnicas</option>
                            <option value="0.75">Sin especializaciones profesionales/especializaciones técnicas</option>
                        </select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="CursoProfesionalRequerido" class="col-3 col-form-label">Cursos profesionales requeridos</label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="txtCursoProfesionalRequerido" name="txtCursoProfesionalRequerido">
                            <option value="0.50">Cuenta con los cursos profesionales (requeridos)</option>
                            <option value="0.25">No cuenta con cursos profesionales (requeridos)</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/ComfuavinavEvaluacionAlistamientoDotacionVuelo">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
