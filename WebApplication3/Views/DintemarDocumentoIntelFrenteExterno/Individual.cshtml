                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbDependenciae" name="cbDependenciae"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Zonanaval" class="col-3 col-form-label"><a href="~/ZonaNaval">Zona Naval</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbZonanavale" name="cbZonanavale"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Pais" class="col-3 col-form-label"><a href="~/PaisUbigeo">País</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbPaise" name="cbPaise"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NotaInfoProd" class="col-3 col-form-label"> Nota de Información Producidas </label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtNotaInfoProde" name="txtNotaInfoProde" placeholder="Nota de Información Producidas" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NotaIntelig" class="col-3 col-form-label">Nota de inteligencia</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtNotaIntelige" name="txtNotaIntelige" placeholder=" Nota de inteligencia" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ApreciacionIntel" class="col-3 col-form-label"> Apreciación de inteligencia  </label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtApreciacionIntele" name="txtApreciacionIntele" placeholder=" Apreciación de inteligencia " required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NotasInfoContra" class="col-3 col-form-label">Notas Información Contrainteligencia </label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtNotasInfoContrae" name="txtNotasInfoContrae" placeholder="Notas Información Contrainteligencia" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ResMensualI" class="col-3 col-form-label"> Resumen mensual de inteligencia </label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtResMensualIe" name="txtResMensualIe" placeholder=" Resumen mensual de inteligencia " required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="BoletinI" class="col-3 col-form-label"> Boletines de información</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtBoletinIe" name="txtBoletinIe" placeholder=" Boletines de información" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Otros" class="col-3 col-form-label">Otros</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtOtrose" name="txtOtrose" placeholder="Otros" required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DintemarDocumentoIntelFrenteExterno">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="listar">
    <p><a href="~/DintemarDocumentoIntelFrenteExterno" class="btn btn-primary">Ir Carga Masiva</a></p>
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaDintemarDocumentoIntelFrenteExterno()" />
    <div class="row align-items-start">
        <div class="col"><select class="form-select form-control-sm" id="cargas" name="cargas"></select></div>
        <div class="col">
            <input type='button' id="btn_search" value="Buscar" class="btn btn-primary">
            <input type='button' id="btn_all" value="Mostrar Todos" class="btn btn-primary">
        </div>
    </div>
    <table class="table table-striped" id="tblDintemarDocumentoIntelFrenteExterno" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>Mes</th>
                <th>Año</th>
                <th>Dependencia </th>
                <th>Zona Naval</th>
                <th>País</th>
                <th>Nota de Información Producidas</th>
                <th>Nota de inteligencia</th>
                <th>Apreciación de inteligencia </th>
                <th>Notas Información Contrainteligencia</th>
                <th>Resumen mensual de inteligencia</th>
                <th>Boletines de información</th>
                <th>Otros</th>
                <th>Código Carga</th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>


@section Scripts{

    <script src="/js/dintemar/dintemar-documentointelfrenteexterno.js" asp-append-version="true"></script>

