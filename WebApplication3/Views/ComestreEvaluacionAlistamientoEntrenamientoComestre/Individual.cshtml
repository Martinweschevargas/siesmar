@{
    ViewData["Title"] = "Evaluacion Alistamiento Entrenamiento Comestre";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1><a href="~/ComestreEvaluacionAlistamientoEntrenamientoComestre/Individual" style="text-decoration:none;">Evaluacion de Alistamiento del Entrenamiento</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="Identificacion" class="col-3 col-form-label"><a href="~/UnidadNaval">Identificacion Unidad Naval</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbIdentificacion" name="cbIdentificacion"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Entrenamiento" class="col-3 col-form-label">Nivel de Entrenamiento</label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="txtEntrenamiento" name="txtEntrenamiento" data-placeholder="Seleccione ...">
                            <option value="1">Básico</option>
                            <option value="2">Intermedio</option>
                            <option value="3">Avanzado</option>
                        </select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Capacidad" class="col-3 col-form-label"><a href="~/CapacidadOperativa">Capacidad Operativa</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbCapacidad" name="cbCapacidad"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Operativa" class="col-3 col-form-label">Tipo de Capacidad Operativa</label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="txtOperativa" name="txtOperativa" data-placeholder="Seleccione ...">
                            <option value="1">Principal</option>
                            <option value="2">Secundaria</option>
                        </select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Ejercicio" class="col-3 col-form-label">Codigo de Ejercicio</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtEjercicio" name="txtEjercicio" placeholder="Codigo de Ejercicio" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Ejercicio" class="col-3 col-form-label"><a href="~/EjercicioEntrenamiento">Ejercicio</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbEjercicio" name="cbEjercicio"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="SubEjercicio" class="col-3 col-form-label"><a href="~/EjercicioEntrenamientoAspecto">Sub-Ejercicio</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbSubEjercicio" name="cbSubEjercicio"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Estado" class="col-3 col-form-label">Peso de cada Sub-Ejercicio</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtEstado" name="txtEstado" placeholder="Peso de cada Sub-Ejercicio" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Calificativo" class="col-3 col-form-label"><a href="~/CalificativoAsignadoEjercicio">Calificativo</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbCalificativo" name="cbCalificativo"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Puntuaje" class="col-3 col-form-label">Puntuaje Obtenido por Ejercicio</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtPuntuaje" name="txtPuntuaje" placeholder="Puntuaje Obtenido por Ejercicio" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Periodo" class="col-3 col-form-label">Fecha del Periodo a Evaluar el Ejercicio</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtPeriodo" name="txtPeriodo" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Realizacion" class="col-3 col-form-label">Fecha de Realizacion del Ejercicio</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtRealizacion" name="txtRealizacion" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Vigencia" class="col-3 col-form-label">Tiempo de Vigencia</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtVigencia" name="txtVigencia" placeholder="Tiempo de Vigencia" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Caducidad" class="col-3 col-form-label">Fecha de Caducidad del Ejercicio</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtCaducidad" name="txtCaducidad" required>
                    </div>
                </div>
            </div>
        </div>
        <div class="row mt-2">
            <div class="col-md-6 text-center">
                <button type="submit" class="btn btn-primary">Grabar</button>
            </div>
            <div class="col-md-6 text-center">
                <a class="btn btn-danger" href="~/ComestreEvaluacionAlistamientoEntrenamientoComestre">Cancelar</a>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
