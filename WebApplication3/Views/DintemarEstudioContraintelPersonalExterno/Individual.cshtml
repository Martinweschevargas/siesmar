@{
    ViewData["Title"] = "Estudios Contrainteligencia Personal Externo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1><a href="~/DintemarEstudioContraintelPersonalExterno/Individual" style="text-decoration:none;">Estudios de Contrainteligencia a PersonaL Externo a la Institución</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>
            <div class="form-group row col-md-6">
                <label for="carga" class="col-3 col-form-label">Carga</label>
                <div class="form-group row col-md-6">
                    <div class="col"><select class="form-select-sm form-control-sm" id="cargasR" name="cargasR"></select></div>
                </div>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="PaisUbigeo" class="col-3 col-form-label"><a href="~/PaisUbigeo" style="text-decoration:none;">País</a> </label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbPaisUbigeo" name="cbPaisUbigeo"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="TipoVinculo" class="col-3 col-form-label"><a href="~/TipoVinculo" style="text-decoration:none;">Tipo Vinculo</a> </label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbTipoVinculo" name="cbTipoVinculo"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Dependencia" class="col-3 col-form-label"><a href="~/Dependencia">Dependencia</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbDependencia" name="cbDependencia"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ComandanciaDep" class="col-3 col-form-label"><a href="~/ComandanciaDependencia">Dirección General</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbComandanciaDep" name="cbComandanciaDep"></select>
                    </div>
                </div>  
                <div class="form-group row col-md-6">
                    <label for="Zonanaval" class="col-3 col-form-label"><a href="~/ZonaNaval">Zona Naval</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbZonanaval" name="cbZonanaval"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NroInvestigContra" class="col-3 col-form-label"> Nro Investigaciones de Contrainteligencia Producidos </label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtNroInvestigContra" name="txtNroInvestigContra" placeholder="Nro Investigaciones de Contrainteligencia Producidos" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="TipoEstudioContra" class="col-3 col-form-label"><a href="~/TipoEstudioContraInteligencia">Tipo de Estudio de Contrainteligencia</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbTipoEstudioContra" name="cbTipoEstudioContra" data-placeholder="Seleccione ..."></select>
                    </div>
                </div>
            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DintemarEstudioContraintelPersonalExterno">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="editar" style="display:none;">
    <form class="form-actualizacion" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <input type="hidden" id="txtCodigo" />
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Editar</h4>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="PaisUbigeo" class="col-3 col-form-label"><a href="~/PaisUbigeo" style="text-decoration:none;">País</a> </label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbPaisUbigeoe" name="cbPaisUbigeoe"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="TipoVinculo" class="col-3 col-form-label"><a href="~/TipoVinculo" style="text-decoration:none;">Tipo Vinculo</a> </label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbTipoVinculoe" name="cbTipoVinculoe"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Dependencia" class="col-3 col-form-label"><a href="~/Dependencia">Dependencia</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbDependenciae" name="cbDependenciae"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ComandanciaDep" class="col-3 col-form-label"><a href="~/ComandanciaDependencia">Dirección General</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbComandanciaDepe" name="cbComandanciaDepe"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Zonanaval" class="col-3 col-form-label"><a href="~/ZonaNaval">Zona Naval</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbZonanavale" name="cbZonanavale"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NroInvestigContra" class="col-3 col-form-label"> Nro Investigaciones de Contrainteligencia Producidos </label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtNroInvestigContrae" name="txtNroInvestigContrae" placeholder="Nro Investigaciones de Contrainteligencia Producidos" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="TipoEstudioContra" class="col-3 col-form-label"><a href="~/TipoEstudioContraInteligencia">Tipo de Estudio de Contrainteligencia</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbTipoEstudioContrae" name="cbTipoEstudioContrae" data-placeholder="Seleccione ..."></select>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DintemarEstudioContraintelPersonalExterno">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="listar">
    <p><a href="~/DintemarEstudioContraintelPersonalExterno" class="btn btn-primary">Ir Carga Masiva</a></p>
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaDintemarEstudioContraintelPersonalExterno()" />
    <div class="row align-items-start">
        <div class="col"><select class="form-select form-control-sm" id="cargas" name="cargas"></select></div>
        <div class="col">
            <input type='button' id="btn_search" value="Buscar" class="btn btn-primary">
            <input type='button' id="btn_all" value="Mostrar Todos" class="btn btn-primary">
        </div>
    </div>
    <table class="table table-striped" id="tblDintemarEstudioContraintelPersonalExterno" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>País</th>
                <th>Tipo Vinculo </th>
                <th>Dependencia </th>
                <th>Zona Naval</th>
                <th>Dirección General</th>
                <th>Nro Investigaciones de Contrainteligencia Producidos</th>
                <th>Tipo de Estudio de Contrainteligencia </th>
                <th>Código Carga</th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>


@section Scripts{

    <script src="/js/dintemar/dintemar-estudiocontraintelpersonalexterno.js" asp-append-version="true"></script>

}
