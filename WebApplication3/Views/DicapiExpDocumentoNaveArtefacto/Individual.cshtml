                        <input type="text" class="form-control form-control-sm" id="txtRazone" name="txtRazone" placeholder="Razon Social" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Tipo" class="col-3 col-form-label"><a href="~/ClaseNave">Tipo de Nave o Artefacto</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbTipoe" name="cbTipoe"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Matricula" class="col-3 col-form-label">Matricula</label>
