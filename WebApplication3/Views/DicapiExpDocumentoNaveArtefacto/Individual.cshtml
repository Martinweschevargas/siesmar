                </div>
                <div class="form-group row col-md-6">
                    <label for="Nacionalidad" class="col-3 col-form-label"><a href="~/PaisUbigeo">Nacionalidad</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbNacionalidade" name="cbNacionalidade"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaAtencion" class="col-3 col-form-label">Fecha de Atencion Solicitud</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaAtencione" name="txtFechaAtencione" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Observacion" class="col-3 col-form-label">Observaciones</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtObservacione" name="txtObservacione" placeholder="Observaciones" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Responsable" class="col-3 col-form-label">Responsable</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtResponsablee" name="txtResponsablee" placeholder="Responsable" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Capitania" class="col-3 col-form-label"><a href="~/Capitania">Capitania de Puerto</a></label>
                    <div class="col-md-9">
