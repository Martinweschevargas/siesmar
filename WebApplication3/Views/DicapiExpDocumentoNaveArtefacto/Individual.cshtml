                    </div>
                </div>
                <div class="row mt-2">
                    <div class="col-md-6 text-center">
                        <button type="submit" class="btn btn-primary">Grabar</button>
                    </div>
                    <div class="col-md-6 text-center">
                        <a class="btn btn-danger" href="~/DicapiExpDocumentoNaveArtefacto">Cancelar</a>
                    </div>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="listar" style="overflow:scroll">
    <p><a href="~/DicapiExpDocumentoNaveArtefacto" class="btn btn-primary">Ir Carga Masiva</a></p>
<input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaDicapiExpDocumentoNaveArtefacto()" />
    <div class="row align-items-start">
        <div class="col"><select class="form-select form-control-sm" id="cargas" name="cargas"></select></div>
        <div class="col">
            <input type='button' id="btn_search" value="Buscar" class="btn btn-primary">
            <input type='button' id="btn_all" value="Mostrar Todos" class="btn btn-primary">
        </div>
    </div>
    <table class="table table-striped" id="tblDicapiExpDocumentoNaveArtefacto" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;overflow:scroll">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>N° Documento</th>
                <th>Fecha de Ingreso Solicitud</th>
                <th>Codigo Procedimiento TUPAM</th>
                <th>Documento Expedido</th>
                <th>Nombre de la Nave o Artefacto</th>
                <th>Propietario</th>
                <th>Razon Social</th>
                <th>Tipo de Nave o Artefacto</th>
                <th>Matricula</th>
                <th>Nacionalidad</th>
                <th>Fecha de Atencion Solicitud</th>
                <th>Observaciones</th>
                <th>Responsable</th>
                <th>Capitania de Puerto</th>
                <th>Código Carga</th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

@section Scripts{
    <script src="/js/dicapi/dicapi-expdocumentonaveartefacto.js" asp-append-version="true"></script>
}
