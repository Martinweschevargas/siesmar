@{
    ViewData["Title"] = "Formato";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-sm-12">
        <h1><a href="~/ComoperguardZarpeNave" style="text-decoration:none;">Zarpe de Naves</a></h1>
        <div class="card">
            <div class="card-header">
                <div class="card-header">
                    <a href="~/ComoperguardZarpeNave/Individual" class="btn btn-primary">Ir Carga Individual</a>
                </div>
            </div>
            <div class="card-body">

                <div class="row">
                    <div class="col-sm-4">
                        <input class="form-control-file" type="file" id="inputExcel" />
                    </div>
                    <div class="col-sm-4">
                        <button class="btn btn-warning" onclick="mostrarDatos()">Mostrar Datos"</button>
                        <button class="btn btn-success" onclick="enviarDatos()">Enviar Datos a BD</button>
                        <label>Numero de Registros : </label><span id="NumRegistros"></span>
                    </div>
                </div>
                <hr />
                <div class="row" style="overflow:scroll">
                    <div class="col-sm-12">
                        <table id="tbData" class="table table-striped">
                            <thead>
                                <tr>
                                    <th>Código</th>
                                    <th>Jefatura De Distrito De Capitanias</th>
                                    <th>Capitania De Puerto</th>
                                    <th>Hora</th>
                                    <th>Dia</th>
                                    <th>Mes Nave/Persona</th>
                                    <th>Año</th>
                                    <th>Puerto de Zarpe</th>
                                    <th>Indicativo Llamada</th>
                                    <th>Nombre</th>
                                    <th>Bandera</th>
                                    <th>Tipo</th>
                                    <th>N° OMI</th>
                                    <th>AB</th>
                                    <th>Agencia Marítima</th>
                                    <th>Pais</th>
                                    <th>Puerto</th>
                                    <th>Tiempo estimado de arribo</th>
                                    <th>Chilenos</th>
                                    <th>Ecuatorianos</th>
                                    <th>Total</th>
                                    <th>Chilenos</th>
                                    <th>Ecuatorianos</th>
                                    <th>Total</th>
                                    <th>Cantidad</th>
                                    <th>Unidad De Medida</th>
                                    <th>Tipo de Carga</th>
                                    <th>Cantidad</th>
                                    <th>Unidad de Medida</th>
                                    <th>Tipo de Carga</th>
                                    <th>Cantidad</th>
                                    <th>Unidad De Medida</th>
                                    <th>Tipo De Carga</th>
                                    <th>Cantidad</th>
                                    <th>Unidad De Medida</th>
                                    <th>Tipo De Carga</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
        </div>

    </div>
</div>
@section Scripts{
    <script src="/js/comoperguard/comoperguard-zarpenave.js" asp-append-version="true"></script>
}