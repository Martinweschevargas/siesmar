@{
    ViewData["Title"] = "Formato";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-sm-12">
        <h1><a href="~/ComoperamaEfectivoNivelEntrenamientoUnidad" style="text-decoration:none;">Efectivos de la Marina Según Nivel de Entrenamiento por Unidades</a></h1>
        <div class="card">
            <div class="card-header">
                <div class="card-header">
                    <a href="~/ComoperamaEfectivoNivelEntrenamientoUnidad/Individual" class="btn btn-primary">Ir Carga Individual</a>
                    <a href="~/ComoperamaEfectivoNivelEntrenamientoUnidad/DownloadFile" class="btn btn-primary">Descargar Formato</a>
                </div>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-sm-4">
                        <input class="form-control" type="file" id="inputExcel" />
                    </div>
                    <div class="col-sm-4">
                        <button class="btn btn-warning" onclick="mostrarDatos()">Vista Previa</button>
                        <button class="btn btn-success" onclick="enviarDatos()">Guardar Formato</button>
                    </div>
                    <div class="col-sm-4">
                        <label>Numero de Registros : </label><span id="NumRegistros"></span>
                    </div>
                </div>
                <hr />
                <div class="row" style="overflow:scroll">
                    <div class="col-sm-12">
                        <table class="table table-striped" id="tbData" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;">
                            <thead>
                                <tr class="colorbg">
                                    <th>Comandancia General</th>
                                    <th>Dependencia / Unidad</th>
                                    <th>Jerarquía </th>
                                    <th>Nivel Elemental</th>
                                    <th>Nivel Básico</th>
                                    <th>Nivel Intermedio </th>
                                    <th>Nivel Avanzado </th>
                                    <th>Nivel Conjunto</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>

        </div>

    </div>
</div>
@section Scripts{
    <script src="/js/comoperama/comoperama-efectivonivelentrenamientounidad.js" asp-append-version="true"></script>
}