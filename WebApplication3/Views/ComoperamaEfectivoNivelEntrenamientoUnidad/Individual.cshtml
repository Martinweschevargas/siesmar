@{
    ViewData["Title"] = "EfectivosdelaMarinaSegúnNiveldeEntrenamientoporUnidades";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1><a href="~/ComoperamaEfectivoNivelEntrenamientoUnidad/Individual" style="text-decoration:none;">Efectivos de la Marina Según Nivel de Entrenamiento por Unidades</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>
            <div class="form-group row col-md-6">
                <label for="carga" class="col-3 col-form-label">Carga</label>
                <div class="form-group row col-md-6">
                    <div class="col"><select class="form-select-sm form-control-sm" id="cargasR" name="cargasR"></select></div>
                </div>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="ComandanciaDependencia" class="col-3 col-form-label"><a href="~/ComandanciaDependencia">Comandancia General</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbComandanciaDependencia" name="cbComandanciaDependencia"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Dependencia" class="col-3 col-form-label"><a href="~/Dependencia">Dependencia / Unidad</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbDependencia" name="cbDependencia"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="GradoPersonal" class="col-3 col-form-label"><a href="~/GradoPersonal">Jerarquía </a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbGradoPersonal" name="cbGradoPersonal"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NivelElemental" class="col-3 col-form-label">Nivel Elemental</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtNivelElemental" name="txtNivelElemental" placeholder="Nivel Elemental" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NivelBasico" class="col-3 col-form-label">Nivel Básico</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtNivelBasico" name="txtNivelBasico" placeholder="Nivel Básico" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NivelIntermedio" class="col-3 col-form-label">Nivel Intermedio </label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtNivelIntermedio" name="txtNivelIntermedio" placeholder="Nivel Intermedio " required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NivelAvanzado" class="col-3 col-form-label">Nivel Avanzado </label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtNivelAvanzado" name="txtNivelAvanzado" placeholder="Nivel Avanzado " required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NivelConjunto" class="col-3 col-form-label">Nivel Conjunto</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtNivelConjunto" name="txtNivelConjunto" placeholder="Nivel Conjunto" required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/ComoperamaEfectivoNivelEntrenamientoUnidad">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
