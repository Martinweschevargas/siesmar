@{
    ViewData["Title"] = "Formato";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-sm-12">
        <h1><a href="~/ComesclaBandaMusico" style="text-decoration:none;">Banda de Músicos</a></h1>
        <div class="card">
            <div class="card-header">
                <div class="card-header">
                    <a href="~/ComesclaBandaMusico/Individual" class="btn btn-primary">Ir Carga Individual</a>
                </div>
            </div>
            <div class="card-body" >

                <div class="row">
                    <div class="col-sm-4">
                        <input class="form-control-file" type="file" id="inputExcel" />
                    </div>
                    <div class="col-sm-4">
                        <button class="btn btn-warning" onclick="mostrarDatos()">Mostrar Datos"</button>
                        <button class="btn btn-success" onclick="enviarDatos()">Enviar Datos a BD</button>
                        <label>Numero de Registros : </label><span id="NumRegistros"></span>
                    </div>
                </div>
                <hr />
                <div class="row" style="overflow:scroll">
                    <div class="col-sm-12">
                        <table id="tbData" class="table table-striped">
                            <thead>
                                <tr>
                                    <th>Código</th>
                                    <th>Tipo deComisión</th>
                                    <th>Evento</th>
                                    <th>Solicitud/Documento De Referencia</th>
                                    <th>Institución/Dependencia Solicitante</th>
                                    <th>Grupo Comisionado </th>
                                    <th>Vestimenta/Uniforme A Usar</th>
                                    <th>Nombre Del Evento A Cubrir</th>
                                    <th>Lugar</th>
                                    <th>Fecha Y Hora De Salida</th>
                                    <th>Fecha Y Hora De Inicio</th>
                                    <th>Fecha Y Hora De Termino</th>
                                    <th>Requerimiento De Movilidad</th>
                                    <th>Novedades/Observaciones</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
        </div>

    </div>
</div>
@section Scripts{
    <script src="/js/comescla/comescla-bandamusicocomescla.js" asp-append-version="true"></script>
}