@{
    ViewData["Title"] = "NúmerodeUnidadesNavales,AeronavalesyPelotonesOperativasdelasFuerzasNavales";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1><a href="~/ComoperpacNumeroUnidadFuerzaNaval/Individual" style="text-decoration:none;">Número de Unidades Navales, Aeronavales y Pelotones Operativas de las Fuerzas Navales</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <select class="form-select form-control-sm" id="cargasR" name="cargasR"></select>
                </div>
                <div class="col-sm-2">
                    <input type="date" class="form-control form-control-sm" id="txtFecha" name="txtFecha" required>
                </div>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="ComandanciaDependencia" class="col-3 col-form-label"><a href="~/ComandanciaDependencia">Comandancia</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbComandanciaDependencia" name="cbComandanciaDependencia"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="UnidadBelica" class="col-3 col-form-label"><a href="~/UnidadBelica">Unidades Bélicas</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbUnidadBelica" name="cbUnidadBelica"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="EstadoOperativo" class="col-3 col-form-label"><a href="~/EstadoOperativo">Estado de Operatividad</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbEstadoOperativo" name="cbEstadoOperativo"></select>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/ComoperpacNumeroUnidadFuerzaNaval">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="editar" style="display:none;">
    <form class="form-actualizacion" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <input type="hidden" id="txtCodigo" />
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Editar</h4>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="ComandanciaDependencia" class="col-3 col-form-label"><a href="~/ComandanciaDependencia">Comandancia</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbComandanciaDependenciae" name="cbComandanciaDependenciae"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="UnidadBelica" class="col-3 col-form-label"><a href="~/UnidadBelica">Unidades Bélicas</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbUnidadBelicae" name="cbUnidadBelicae"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="EstadoOperativo" class="col-3 col-form-label"><a href="~/EstadoOperativo">Estado de Operatividad</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbEstadoOperativoe" name="cbEstadoOperativoe"></select>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/ComoperpacNumeroUnidadFuerzaNaval">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="listar" style="overflow:scroll">
    <p><a href="~/ComoperpacNumeroUnidadFuerzaNaval" class="btn btn-primary">Ir Carga Masiva</a></p>
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaComoperpacNumeroUnidadFuerzaNaval()" />
    <div class="row align-items-start">
        <div class="col">
            <select class="form-select form-control-sm" id="cargas" name="cargas"></select>
        </div>
        <div class="col">
            <button class="btn btn-primary" id="btn_search">Buscar</button>
            <button class="btn btn-primary" id="btn_all">Mostrar Todos</button>
            <button class="btn btn-danger" onclick="eliminarCarga()">Eliminar Carga</button>
        </div>
    </div>
    <table class="table table-striped" id="tblComoperpacNumeroUnidadFuerzaNaval" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>Comandancia</th>
                <th>Unidades Bélicas</th>
                <th>Estado de Operatividad</th>
                <th>Código Carga</th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

@section Scripts{
    <script src="/js/comoperpac/comoperpac-numerounidadfuerzanaval.js" asp-append-version="true"></script>
}
