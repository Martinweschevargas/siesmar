                </div>
                <div class="form-group row col-md-6">
                    <label for="TipoProceso" class="col-3 col-form-label"><a href="~/TipoProceso">Tipo de Proceso</a></label>
                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbTipoProcesoe" name="cbTipoProcesoe"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="MontoContrato" class="col-3 col-form-label">Monto de Contrato</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtMontoContratoe" name="txtMontoContratoe" placeholder="Monto de Contrato" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaInicioObraServicio" class="col-3 col-form-label">Fecha inicio de la Obra / Servicio</label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaInicioObraServicioe" name="txtFechaInicioObraServicioe" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="FechaTerminoEstimada" class="col-3 col-form-label">Fecha de Término Estimada </label>
                    <div class="col-md-9">
                        <input type="date" class="form-control form-control-sm" id="txtFechaTerminoEstimadae" name="txtFechaTerminoEstimadae" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="PorcentajeAvanceFisico" class="col-3 col-form-label">Avance Físico </label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtPorcentajeAvanceFisicoe" name="txtPorcentajeAvanceFisicoe" placeholder="Avance Físico " required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DibinfraterInspeccionObraServicioPrestado">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="listar" style="overflow:scroll">
    <p><a href="~/DibinfraterInspeccionObraServicioPrestado" class="btn btn-primary">Ir Carga Masiva</a></p>
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaDibinfraterInspeccionObraServicioPrestado()" />
    <div class="row align-items-start mb-4">
        <div class="col">
            <select class="form-select form-control-sm" id="cargas" name="cargas"></select>
        </div>
        <div class="col">
            <button class="btn btn-primary" id="btn_search">Buscar</button>
            <button class="btn btn-primary" id="btn_all">Mostrar Todos</button>
            <button class="btn btn-danger" onclick="eliminarCarga()">Eliminar Carga</button>
        </div>
    </div>
    <hr />
    <table class="table table-striped" id="tblDibinfraterInspeccionObraServicioPrestado" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;overflow:scroll">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>Solicitud</th>
                <th>Nombre de Obra </th>
                <th>Área Usuaria(UU.DD)</th>
                <th>Zona Naval</th>
                <th>Estado Solicitud</th>
                <th>Identificación del Contrato</th>
                <th>Tipo de Obra o Servicio</th>
                <th>Tipo de Proceso</th>
                <th>Monto de Contrato</th>
                <th>Fecha inicio de la Obra / Servicio</th>
                <th>Fecha de Término Estimada </th>
                <th>Avance Físico </th>
                <th>Código Carga</th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

@section Scripts{
    <script src="/js/dibinfrater/dibinfrater-inspeccionobraservicioprestado.js" asp-append-version="true"></script>
}
