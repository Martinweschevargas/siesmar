                    <div class="col-md-9">
                        <select class="form-control form-select-sm" id="cbZonaNavale" name="cbZonaNavale"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="EstadoSolicitud" class="col-3 col-form-label">Estado Solicitud</label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="txtEstadoSolicitude" name="txtEstadoSolicitude" data-placeholder="Seleccione ...">
                            <option value="1">Ejecución</option>
                            <option value="2">Se hizo Inspección</option>
                            <option value="3">Paralizada</option>
                            <option value="4">Se rechazó la solicitud</option>
                            <option value="5">Liquidación</option>
