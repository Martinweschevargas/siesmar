@{
    ViewData["Title"] = "Alistamiento Integral de la Comandancia de Ciberdefensa";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1><a href="~/ComciberdefAlistamientoIntegComanCiberdefensa/Individual" style="text-decoration:none;">Alistamiento Integral de la Comandancia de Ciberdefensa</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>
            <div class="form-group row col-md-6">
                <label for="carga" class="col-3 col-form-label">Carga</label>
                <div class="form-group row col-md-6">
                    <div class="col"><select class="form-select-sm form-control-sm" id="cargasR" name="cargasR"></select></div>
                </div>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="AnioAlistamiento" class="col-3 col-form-label">Año </label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtAnioAlistamiento" name="txtAnioAlistamiento" placeholder="Año " required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="SemestreAlistamiento" class="col-3 col-form-label">Semestre </label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtSemestreAlistamiento" name="txtSemestreAlistamiento" placeholder="Semestre " required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="AlistamientoPersonal" class="col-3 col-form-label">Alistamiento de Personal</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtAlistamientoPersonal" name="txtAlistamientoPersonal" placeholder="Alistamiento de Personal" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="AlistamientoEntretenimiento" class="col-3 col-form-label">Alistamiento de Entrenamiento</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtAlistamientoEntretenimiento" name="txtAlistamientoEntretenimiento" placeholder="Alistamiento de Entrenamiento" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="AlistamientoMaterial" class="col-3 col-form-label">Alistamiento de Material</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtAlistamientoMaterial" name="txtAlistamientoMaterial" placeholder="Alistamiento de Material" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="AlistamientoLogistico" class="col-3 col-form-label">Alistamiento Logístico </label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtAlistamientoLogistico" name="txtAlistamientoLogistico" placeholder="Alistamiento Logístico " required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/ComciberdefAlistamientoIntegComanCiberdefensa">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="editar" style="display:none;">
    <form class="form-actualizacion" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <input type="hidden" id="txtCodigo" />
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Editar</h4>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="AnioAlistamiento" class="col-3 col-form-label">Año </label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtAnioAlistamientoe" name="txtAnioAlistamientoe" placeholder="Año " required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="SemestreAlistamiento" class="col-3 col-form-label">Semestre </label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtSemestreAlistamientoe" name="txtSemestreAlistamientoe" placeholder="Semestre " required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="AlistamientoPersonal" class="col-3 col-form-label">Alistamiento de Personal</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtAlistamientoPersonale" name="txtAlistamientoPersonale" placeholder="Alistamiento de Personal" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="AlistamientoEntretenimiento" class="col-3 col-form-label">Alistamiento de Entrenamiento</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtAlistamientoEntretenimientoe" name="txtAlistamientoEntretenimientoe" placeholder="Alistamiento de Entrenamiento" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="AlistamientoMaterial" class="col-3 col-form-label">Alistamiento de Material</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtAlistamientoMateriale" name="txtAlistamientoMateriale" placeholder="Alistamiento de Material" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="AlistamientoLogistico" class="col-3 col-form-label">Alistamiento Logístico </label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtAlistamientoLogisticoe" name="txtAlistamientoLogisticoe" placeholder="Alistamiento Logístico " required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/ComciberdefAlistamientoIntegComanCiberdefensa">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="listar" style="overflow:scroll">
    <p><a href="~/ComciberdefAlistamientoIntegComanCiberdefensa" class="btn btn-primary">Ir Carga Masiva</a></p>
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaComciberdefAlistamientoIntegComanCiberdefensa()" />
    <div class="row align-items-start">
        <div class="col"><select class="form-select form-control-sm" id="cargas" name="cargas"></select></div>
        <div class="col">
            <input type='button' id="btn_search" value="Buscar" class="btn btn-primary">
            <input type='button' id="btn_all" value="Mostrar Todos" class="btn btn-primary">
        </div>
    </div>
    <table class="table table-striped" id="tblComciberdefAlistamientoIntegComanCiberdefensa" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;overflow:scroll">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>Año </th>
                <th>Semestre </th>
                <th>Alistamiento de Personal</th>
                <th>Alistamiento de Entrenamiento</th>
                <th>Alistamiento de Material</th>
                <th>Alistamiento Logístico </th>
                <th>Código Carga </th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

@section Scripts{
    <script src="/js/comciberdef/comciberdef-alistamientoicciberdefensa.js" asp-append-version="true"></script>
}
