@{
    ViewData["Title"] = "Producción Documentos Contrainteligencia";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1><a href="~/DintemarProduccionDocumentosContraintel/Individual" style="text-decoration:none;">Producción de Documentos de Contrainteligencia</a></h1>
<div id="nuevo" style="display:none;">
    <form class="needs-validation" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Nuevo</h4>
            </div>
            <div class="form-group row col-md-6">
                <label for="carga" class="col-3 col-form-label">Carga</label>
                <div class="form-group row col-md-6">
                    <div class="col"><select class="form-select-sm form-control-sm" id="cargasR" name="cargasR"></select></div>
                </div>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="Mes" class="col-3 col-form-label"><a href="~/Mes">Mes</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbMes" name="cbMes"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Anio" class="col-3 col-form-label">Año</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtAnio" name="txtAnio" placeholder="Año" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Dependencia" class="col-3 col-form-label"><a href="~/Dependencia">Dependencia</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbDependencia" name="cbDependencia"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ComandanciaDep" class="col-3 col-form-label"><a href="~/ComandanciaDependencia">Dirección General</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbComandanciaDep" name="cbComandanciaDep"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Zonanaval" class="col-3 col-form-label"><a href="~/ZonaNaval">Zona Naval</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbZonanaval" name="cbZonanaval"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NroInfoComb" class="col-3 col-form-label">Nro Notas de Información de Contrainteligencia Producidos</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtNroInfoComb" name="txtNroInfoComb" placeholder="Nro Notas de Información de Contrainteligencia Producidos" required>
                    </div>
                </div>               
                <div class="form-group row col-md-6">
                    <label for="NroNotasContra" class="col-3 col-form-label"> Nro Notas de Contrainteligencia Producidos</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtNroNotasContra" name="txtNroNotasContra" placeholder="Nro Notas de Contrainteligencia Producidos" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NroApelacionContra" class="col-3 col-form-label"> Nro Apreciaciones de Contrainteligencia Producidose</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtNroApelacionContra" name="txtNroApelacionContra" placeholder="Nro Apreciaciones de Contrainteligencia Producidose" required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DintemarProduccionDocumentosContraintel">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="editar" style="display:none;">
    <form class="form-actualizacion" novalidate style="font-family: 'Century Gothic'; font-size:14px;">
        <input type="hidden" id="txtCodigo" />
        <div class="card" style="width: 100%;">
            <div class="card-header nuevo">
                <h4>Editar</h4>
            </div>
            <div class="row mt-2">
                <div class="form-group row col-md-6">
                    <label for="Mes" class="col-3 col-form-label"><a href="~/Mes">Mes</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbMese" name="cbMese"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Anio" class="col-3 col-form-label">Año</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtAnioe" name="txtAnioe" placeholder="Año" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Dependencia" class="col-3 col-form-label"><a href="~/Dependencia">Dependencia</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbDependenciae" name="cbDependenciae"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="ComandanciaDep" class="col-3 col-form-label"><a href="~/ComandanciaDependencia">Dirección General</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbComandanciaDepe" name="cbComandanciaDepe"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="Zonanaval" class="col-3 col-form-label"><a href="~/ZonaNaval">Zona Naval</a></label>
                    <div class="col-md-9">
                        <select class="form-select form-select-sm" id="cbZonanavale" name="cbZonanavale"></select>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NroInfoComb" class="col-3 col-form-label">Nro Notas de Información de Contrainteligencia Producidos</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtNroInfoCombe" name="txtNroInfoCombe" placeholder="Nro Notas de Información de Contrainteligencia Producidos" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NroNotasContra" class="col-3 col-form-label"> Nro Notas de Contrainteligencia Producidos</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtNroNotasContrae" name="txtNroNotasContrae" placeholder="Nro Notas de Contrainteligencia Producidos" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="NroApelacionContra" class="col-3 col-form-label"> Nro Apreciaciones de Contrainteligencia Producidose</label>
                    <div class="col-md-9">
                        <input type="number" class="form-control form-control-sm" id="txtNroApelacionContrae" name="txtNroApelacionContrae" placeholder="Nro Apreciaciones de Contrainteligencia Producidose" required>
                    </div>
                </div>

            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/DintemarProduccionDocumentosContraintel">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>
<div id="listar">
    <p><a href="~/DintemarProduccionDocumentosContraintel" class="btn btn-primary">Ir Carga Masiva</a></p>
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaDintemarProduccionDocumentosContraintel()" />
    <div class="row align-items-start">
        <div class="col"><select class="form-select form-control-sm" id="cargas" name="cargas"></select></div>
        <div class="col">
            <input type='button' id="btn_search" value="Buscar" class="btn btn-primary">
            <input type='button' id="btn_all" value="Mostrar Todos" class="btn btn-primary">
        </div>
    </div>
    <table class="table table-striped" id="tblDintemarProduccionDocumentosContraintel" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>Mes</th>
                <th>Año </th>
                <th>Dependencia </th>
                <th>Dirección General</th>
                <th>Zona Naval </th>
                <th>Nro Notas de Información de Contrainteligencia Producidos </th>
                <th>Nro Notas de Contrainteligencia Producidos </th>
                <th>Nro Apreciaciones de Contrainteligencia Producidose </th>
                <th>Código Carga</th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>


@section Scripts{

    <script src="/js/dintemar/dintemar-producciondocumentoscontraintel.js" asp-append-version="true"></script>

}
