                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="txtBAPPastazae" class="col-3 col-form-label">BAP Pastaza</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtBAPPastazae" name="txtBAPPastazae" placeholder="BAP Pastaza" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="txtPersonale" class="col-3 col-form-label">Personal</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtPersonale" name="txtPersonale" placeholder="Personal" required>
                    </div>
                </div>
            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/TiempoServicioExperiencia">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>

<div id="listar" style="overflow:scroll">
    <input type="button" value="NUEVO REGISTRO" class="nuevo" onclick="nuevaTiempoServicioExperiencia()" />
    <table class="table table-striped" id="tblTiempoServicioExperiencias" cellspacing="0" width="100%" style="font-family: Century Gothic; font-size:14px;">
        <thead>
            <tr class="colorbg">
                <th>Código</th>
                <th>BAP Amazonas</th>
                <th>BAP Loreto</th>
                <th>BAP Marañón</th>
                <th>BAP Ucayali</th>
                <th>BAP Clavero</th>
                <th>BAP Castillo</th>
                <th>BAP Morona</th>
                <th>BAP Corrientes</th>
                <th>BAP Pastaza</th>
                <th>Personal</th>
                <th colspan="2" class="text-center">Acciones</th>
                <th class="ocultar"></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

@section Scripts{
    <script src="/js/tiemposervicioexperiencia.js" asp-append-version="true"></script>
