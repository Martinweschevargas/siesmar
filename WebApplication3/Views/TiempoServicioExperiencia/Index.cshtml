                        <input type="text" class="form-control form-control-sm" id="txtBAPAmazonas" name="txtBAPAmazonas" placeholder="BAP Amazonas" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="txtBAPLoreto" class="col-3 col-form-label">BAP Loreto</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtBAPLoreto" name="txtBAPLoreto" placeholder="BAP Loreto" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="txtBAPMaranon" class="col-3 col-form-label">BAP Marañón</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtBAPMaranon" name="txtBAPMaranon" placeholder="BAP Maranon" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="txtBAPUcayali" class="col-3 col-form-label">BAP Ucayali</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtBAPUcayali" name="txtBAPUcayali" placeholder="BAP Ucayali" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="txtBAPClavero" class="col-3 col-form-label">BAP Clavero</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtBAPClavero" name="txtBAPClavero" placeholder="BAP Clavero" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="txtBAPCastillo" class="col-3 col-form-label">BAP Castillo</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtBAPCastillo" name="txtBAPCastillo" placeholder="BAP Castillo" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="txtBAPMorona" class="col-3 col-form-label">BAP Morona</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtBAPMorona" name="txtBAPMorona" placeholder="BAP Morona" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="txtBAPCorrientes" class="col-3 col-form-label">BAP Corrientes</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtBAPCorrientes" name="txtBAPCorrientes" placeholder="BAP Corrientes" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="txtBAPPastaza" class="col-3 col-form-label">BAP Pastaza</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtBAPPastaza" name="txtBAPPastaza" placeholder="BAP Pastaza" required>
                    </div>
                </div>
                <div class="form-group row col-md-6">
                    <label for="txtPersonal" class="col-3 col-form-label">Personal</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control form-control-sm" id="txtPersonal" name="txtPersonal" placeholder="Personal" required>
                    </div>
                </div>
            </div>
            <div class="row mt-2">
                <div class="col-md-6 text-center">
                    <button type="submit" class="btn btn-primary">Grabar</button>
                </div>
                <div class="col-md-6 text-center">
                    <a class="btn btn-danger" href="~/TiempoServicioExperiencia">Cancelar</a>
                </div>
            </div>
        </div>
    </form>
    <div class="loader" id="loader-6" style="display:none;">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
    </div>
</div>

